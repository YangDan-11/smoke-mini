.wrapper {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 32rpx;
}

.content {
    flex: 1;
    text-align: center;
}

.prev-btn, .next-btn {
    background-color: #6190e8;
    border: 2rpx solid #4a81e7;
    color: #FFF;
    font-size: 26rpx;
    width: 140rpx;
}

.prev-btn[disabled], .next-btn[disabled] {
    color: #7f7f7f;
    border: 2rpx solid #7f7f7f;
}